{
"manifest_version": 2,
    "name": "Autocamel",
    "description": "More camels in your browser",
    "version": "0.25",
    "icons": { "48": "icon-48.png",
          "128": "icon-128.png" },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}